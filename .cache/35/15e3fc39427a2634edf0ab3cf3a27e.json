rated":{"line":102,"column":0},"source":"src/components/Account/ScreenConnecting.js","original":{"line":112,"column":31}}],"sources":{"src/components/Account/ScreenConnecting.js":"import React from 'react'\nimport PropTypes from 'prop-types'\nimport { keyframes } from 'styled-components'\nimport { GU, useTheme, textStyle, Link } from '@aragon/ui'\nimport { getProviderFromUseWalletId } from 'use-wallet'\n\nimport loadingRing from './assets/loading-ring.svg'\n\nconst spin = keyframes`\n  from {\n    transform: rotate(0deg);\n  }\n  to {\n    transform: rotate(360deg);\n  }\n`\n\nfunction ScreenConnecting({ onCancel, providerId }) {\n  const theme = useTheme()\n  const provider = getProviderFromUseWalletId(providerId)\n  return (\n    <section\n      css={`\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        justify-content: center;\n        padding: ${2 * GU}px;\n        height: 100%;\n      `}\n    >\n      <div\n        css={`\n          flex-grow: 1;\n          display: flex;\n          flex-direction: column;\n          align-items: center;\n          justify-content: center;\n          text-align: center;\n        `}\n      >\n        <div\n          css={`\n            position: relative;\n            width: ${10.5 * GU}px;\n            height: ${10.5 * GU}px;\n          `}\n        >\n          <div\n            css={`\n              position: absolute;\n              top: 0;\n              left: 0;\n              right: 0;\n              bottom: 0;\n              background: url(${loadingRing}) no-repeat 0 0;\n              animation-duration: 1s;\n              animation-iteration-count: infinite;\n              animation-timing-function: linear;\n              animation-name: ${spin};\n              // prevents flickering on Firefox\n              backface-visibility: hidden;\n            `}\n          />\n          <div\n            css={`\n              position: absolute;\n              top: 0;\n              left: 0;\n              right: 0;\n              bottom: 0;\n              background: 50% 50% / auto ${5 * GU}px no-repeat\n                url(${provider.image});\n            `}\n          />\n        </div>\n        <h1\n          css={`\n            padding-top: ${2 * GU}px;\n            ${textStyle('body1')};\n            font-weight: 600;\n          `}\n        >\n          Connecting to {provider.name}\n        </h1>\n        <p\n          css={`\n            width: ${36 * GU}px;\n            color: ${theme.surfaceContentSecondary};\n          `}\n        >\n          Log into {provider?.id || 'Unknown'}. You may be temporarily\n          redirected to a new screen.\n        </p>\n      </div>\n      <div\n        css={`\n          flex-grow: 0;\n        `}\n      >\n        <Link onClick={onCancel}>Cancel</Link>\n      </div>\n    </section>\n  )\n}\n\nScreenConnecting.propTypes = {\n  providerId: PropTypes.string,\n  onCancel: PropTypes.func.isRequired,\n}\n\nexport default ScreenConnecting\n"},"lineCount":null}},"error":null,"hash":"f295bad8ef496b9574c87b2651bfc5df","cacheData":{"env":{}}}
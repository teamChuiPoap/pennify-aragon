ated":{"line":63,"column":28},"source":"src/apps/finance/components/ToggleContent.js","original":{"line":60,"column":28}},{"generated":{"line":64,"column":0},"source":"src/apps/finance/components/ToggleContent.js","original":{"line":60,"column":28}}],"sources":{"src/apps/finance/components/ToggleContent.js":"import React, { useState } from 'react'\nimport { ButtonBase, GU, IconUp, RADIUS, springs, useTheme } from '@aragon/ui'\nimport { Transition, animated } from 'react-spring/renderprops'\n\nconst AnimatedDiv = animated.div\n\nfunction ToggleContent({ label, children }) {\n  const theme = useTheme()\n  const [opened, setOpened] = useState()\n  return (\n    <React.Fragment>\n      <ButtonBase\n        onClick={() => setOpened(opened => !opened)}\n        focusRingRadius={RADIUS}\n        focusRingSpacing={0.5 * GU}\n        css={`\n          display: flex;\n          align-items: center;\n          width: calc(100%);\n          &:active {\n            color: ${theme.surfaceContentSecondary};\n          }\n        `}\n      >\n        {label}{' '}\n        <IconUp\n          size=\"small\"\n          css={`\n            position: relative;\n            top: -1px;\n            margin-left: ${1 * GU}px;\n            transform-origin: 50% 50%;\n            transform: rotate(${opened ? 180 : 0}deg);\n            transition: transform 200ms ease-in-out;\n          `}\n        />\n      </ButtonBase>\n\n      <Transition\n        items={opened}\n        config={springs.swift}\n        from={{ height: 0, opacity: 0 }}\n        enter={{ height: 'auto', opacity: 1 }}\n        leave={{ height: 0, opacity: 0 }}\n        native\n      >\n        {show =>\n          show &&\n          (props => (\n            <AnimatedDiv style={props} css=\"overflow: hidden\">\n              {children}\n            </AnimatedDiv>\n          ))\n        }\n      </Transition>\n    </React.Fragment>\n  )\n}\n\nexport default ToggleContent\n"},"lineCount":null}},"error":null,"hash":"01a9438f21bf33
"src/apps/voting/components/VoteCard/VoteCardGroup.js":"import React from 'react'\nimport {\n  CardLayout,\n  Tag,\n  GU,\n  textStyle,\n  unselectable,\n  useTheme,\n  useViewport,\n} from '@aragon/ui'\n\nconst VoteCardGroup = ({ title, count, children }) => {\n  const theme = useTheme()\n  const { below } = useViewport()\n  const compactMode = below('medium')\n  const rowHeight = compactMode ? null : 294\n\n  return (\n    <section>\n      <h2\n        css={`\n          display: flex;\n          align-items: center;\n          margin-bottom: ${3 * GU}px;\n          ${compactMode ? `padding: 0 ${2 * GU}px;` : ''}\n          ${unselectable};\n        `}\n      >\n        <div\n          css={`\n            ${textStyle('body2')};\n            color: ${theme.content};\n          `}\n        >\n          {title}\n        </div>\n        <span\n          css={`\n            margin-left: ${1 * GU}px;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n          `}\n        >\n          <Tag limitDigits={4} label={count} size=\"small\" />\n        </span>\n      </h2>\n      <CardLayout columnWidthMin={30 * GU} rowHeight={rowHeight}>\n        {children}\n      </CardLayout>\n    </section>\n  )\n}\n\nexport default VoteCardGroup\n"},"lineCount":null}},"error":null,"hash":"918a7242365219ba160783632fd725aa","cacheData":{"env":{}}}